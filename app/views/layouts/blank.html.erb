<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <title><%if defined? @title%><%=@title%><%else%><%=Statics.nombre_app%><%end%></title>
    <link rel="icon" type="image/x-icon" href="<%= Statics.url_public_img%>favicon.ico">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="shortcut icon" href="assets/img/favicon.png" type="image/x-icon">
    <!-- Inicio Fonts -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,400,600,700,300" rel="stylesheet" type="text/css">
    <!-- Fin Fonts -->
     <!-- Inicio CSS-->
    <link rel="stylesheet" type="text/css" href="<%= Statics.url_bower%>bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="<%= Statics.url_bower%>font-awesome/css/font-awesome.min.css" />
    <!-- Inicio Beyond-->
    <link id="beyond-link" href="<%= Statics.url%>beyond/assets/css/beyond.min.css" rel="stylesheet" />
    <link href="<%= Statics.url%>beyond/assets/css/beyond.min.css" rel="stylesheet" id="beyond-link" />
    <link href="<%= Statics.url%>beyond/assets/css/demo.min.css" rel="stylesheet" />
    <link href="<%= Statics.url%>beyond/assets/css/animate.min.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="<%= Statics.url_public_css%>styles.css" />
    <link id="skin-link" href="" rel="stylesheet" type="text/css" />
    <!--Skin Script: Place this script in head to load scripts for skins and rtl support-->
    <script src="<%= Statics.url%>beyond/assets/js/skins.js"></script>
    <!-- Inicio Load_CSS-->
    <%= load_css %>
    <!-- Fin CSS-->
  </head>
  <body  <% if defined? @body_error %>class="body-404"<% end %>>
      <!-- Inicio Yield-->
      <%= yield %>
      <!-- Fin Yield-->
    <!-- Inicio JS-->
      <script src="<%= Statics.url_bower%>jquery/dist/jquery.min.js" type="text/javascript"></script>
      <script src="<%= Statics.url_bower%>bootstrap/dist/js/bootstrap.min.js" type="text/javascript"></script>
      <script src="<%= Statics.url_bower%>underscore/underscore-min.js" type="text/javascript"></script>
      <script src="<%= Statics.url%>swp/js/mootools-core.min.js" type="text/javascript"></script>
      <script src="<%= Statics.url%>swp/js/mootools.min.js" type="text/javascript"></script>
      <script src="<%= Statics.url%>swp/js/mootools-interfaces.min.js" type="text/javascript"></script>
      <script src="<%= Statics.url%>assets/site/js/app.js" type="text/javascript"></script>
      <!-- Inicio Beyond-->
      <script src="<%= Statics.url%>beyond/assets/js/slimscroll/jquery.slimscroll.min.js"></script>
      <script src="<%= Statics.url%>beyond/assets/js/beyond.js"></script>
      <!-- Inicio Load_JS-->
      <%= load_js %>
      <!-- Fin JS-->
  </body>
</html>
